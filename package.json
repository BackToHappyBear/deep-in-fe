{
    "name": "deep-in-fe",
    "version": "1.0.0",
    "description": "Store my code to explore the javascript mystery",
    "private": true,
    "main": "index.js",
    "author": "YuTengjing",
    "license": "MIT",
    "scripts": {
        "test": "mocha",
        "coverage": "nyc --reporter=lcov yarn test && codecov",
        "test-report": "mocha --reporter mochawesome",
        "lint": "eslint -c .eslintrc.js --ext .js {src,test}/**/*.js",
        "clean": "rimraf -rf .nyc_output coverage mochawesome-report"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "{src,test}/**/*.js": [
            "eslint -c .eslintrc.js"
        ],
        "*.{js,json,md}": [
            "prettier --write"
        ]
    },
    "dependencies": {},
    "devDependencies": {
        "babel-eslint": "^10.0.3",
        "codecov": "^3.6.5",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "husky": "^4.2.1",
        "intelli-espower-loader": "^1.0.1",
        "lint-staged": "^10.0.7",
        "mocha": "^7.0.1",
        "mochawesome": "^4.1.0",
        "nyc": "^15.0.0",
        "power-assert": "^1.6.1",
        "prettier": "^1.18.2",
        "promises-aplus-tests": "^2.1.2",
        "rimraf": "^3.0.2"
    }
}
