{
    "name": "deep-in-fe",
    "version": "1.0.0",
    "description": "Store my code to explore the javascript mystery.",
    "private": true,
    "main": "index.js",
    "author": "YuTengjing",
    "license": "MIT",
    "scripts": {
        "test": "mocha",
        "coverage": "nyc --reporter=lcov yarn test && codecov",
        "coverage-local": "mocha --reporter mochawesome",
        "lint": "eslint -c .eslintrc --ext .js ./{src,test}/**/*.js",
        "clean": "rimraf -rf ./.nyc_output ./coverage ./mochawesome-report"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "{src,test}/**/*.js": [
            "eslint -c .eslintrc"
        ],
        "**/*.{js,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/core": "^7.7.7",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-transform-runtime": "^7.7.6",
        "@babel/preset-env": "^7.7.7",
        "@babel/register": "^7.7.7",
        "@babel/runtime": "^7.7.7",
        "babel-eslint": "^10.0.3",
        "babel-preset-power-assert": "^3.0.0",
        "co": "^4.6.0",
        "codecov": "^3.5.0",
        "core-js": "^3.6.1",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-prettier": "^3.1.2",
        "husky": "^3.0.4",
        "lint-staged": "^9.2.5",
        "mocha": "^6.1.3",
        "mochawesome": "^4.1.0",
        "nyc": "^15.0.0",
        "power-assert": "^1.6.1",
        "prettier": "^1.18.2",
        "promises-aplus-tests": "^2.1.2",
        "rimraf": "^3.0.0"
    }
}
