{
    "name": "deep-in-fe",
    "version": "1.0.0",
    "description": "Store my code to explore the javascript mystery.",
    "private": true,
    "main": "index.js",
    "author": "YuTengjing",
    "license": "MIT",
    "scripts": {
        "test": "mocha -r intelli-espower-loader",
        "coverage": "nyc --reporter=lcov yarn test && codecov",
        "lint": "eslint -c .eslintrc --ext .js ./{src,test}/**/*.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "{src,test}/**/*.js": [
            "eslint -c .eslintrc"
        ],
        "**/*.{js,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {},
    "devDependencies": {
        "codecov": "^3.5.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-plugin-import": "^2.19.1",
        "husky": "^3.0.4",
        "intelli-espower-loader": "^1.0.1",
        "lint-staged": "^9.2.5",
        "mocha": "^6.1.3",
        "nyc": "^15.0.0",
        "power-assert": "^1.6.1",
        "prettier": "^1.18.2"
    }
}
