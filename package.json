{
    "name": "javascript-code-lab",
    "version": "1.0.0",
    "description": "Store my code to explore the javascript mystery.",
    "private": true,
    "main": "index.js",
    "scripts": {
        "test": "mocha --require intelli-espower-loader",
        "lint": "eslint src/",
        "cov": "istanbul cover ./node_modules/mocha/bin/_mocha && npx codecov"
    },
    "keywords": [
        "javascript",
        "tricks",
        "tutorial",
        "code",
        "mystery"
    ],
    "author": "YuTengjing",
    "license": "ISC",
    "devDependencies": {
        "codecov": "^3.5.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-plugin-import": "^2.19.1",
        "husky": "^3.0.4",
        "intelli-espower-loader": "^1.0.1",
        "istanbul": "^0.4.5",
        "lint-staged": "^9.2.5",
        "mocha": "^6.1.3",
        "nyc": "^15.0.0",
        "power-assert": "^1.6.1",
        "prettier": "^1.18.2"
    },
    "dependencies": {},
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "{src,test}/**/*.js": [
            "eslint"
        ],
        "**/*.{js,md}": [
            "prettier",
            "git add"
        ]
    }
}
