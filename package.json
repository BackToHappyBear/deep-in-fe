{
    "name": "deep-in-fe",
    "version": "1.0.0",
    "description": "Store my code to explore the javascript mystery.",
    "private": true,
    "main": "index.js",
    "author": "YuTengjing",
    "license": "MIT",
    "scripts": {
        "test": "mocha",
        "coverage": "nyc --reporter=lcov yarn test && codecov",
        "test-report": "mocha --reporter mochawesome",
        "lint": "eslint -c .eslintrc --ext .js,.ts {src,test}/**/*.{js,ts}",
        "clean": "rimraf -rf .nyc_output coverage mochawesome-report"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "{src,test}/**/*.{js,ts}": [
            "eslint -c .eslintrc"
        ],
        "**/*.{js,ts,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.8.3",
        "@babel/preset-env": "^7.8.3",
        "@babel/preset-typescript": "^7.8.3",
        "@babel/register": "^7.8.3",
        "@babel/runtime": "^7.8.3",
        "babel-eslint": "^10.0.3",
        "babel-preset-power-assert": "^3.0.0",
        "codecov": "^3.5.0",
        "core-js": "^3.6.4",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.20.0",
        "eslint-plugin-prettier": "^3.1.2",
        "husky": "^4.0.10",
        "lint-staged": "^9.2.5",
        "mocha": "^7.0.0",
        "mochawesome": "^4.1.0",
        "nyc": "^15.0.0",
        "power-assert": "^1.6.1",
        "prettier": "^1.18.2",
        "promises-aplus-tests": "^2.1.2",
        "rimraf": "^3.0.0",
        "typescript": "^3.7.5"
    },
    "dependencies": {}
}
